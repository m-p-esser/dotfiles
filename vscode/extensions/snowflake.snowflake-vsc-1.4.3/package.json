{
	"name": "snowflake-vsc",
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "static/snowflake.png",
	"private": true,
	"displayName": "Snowflake",
	"homepage": "https://snowflake.com",
	"description": "Connect to Snowflake, write and execute sql queries, and view results without leaving VS Code.",
	"version": "1.4.3",
	"engines": {
		"vscode": "^1.78.0",
		"node": "~16.17.0",
		"pnpm": "^8.3.1"
	},
	"tags": [
		"Snowflake",
		"SQL"
	],
	"categories": [
		"Programming Languages",
		"Data Science",
		"Other"
	],
	"publisher": "snowflake",
	"main": "extension.js",
	"bugs": {
		"url": "https://community.snowflake.com/s/case-submission-form"
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm",
		"bundle": "webpack --mode production && webpack --mode production --config ./webpack.config.views.js && cmd/attribution",
		"build": "rm -rf dist && mkdir -p build && pnpm run bundle && cmd/build",
		"watch": "webpack --mode development --watch",
		"devserver": "webpack serve --mode development --config ./webpack.config.views.js",
		"compile-tests": "swc ./src/test/runTest.ts -d dist -C module.type=commonjs && swc ./src/test/suite/index.ts -d dist -C module.type=commonjs",
		"format": "prettier 'src/**/*.js' 'src/**/*.jsx' 'src/**/*.ts' 'src/**/*.tsx' './package.json' '.swcrc' './*.js' --no-error-on-unmatched-pattern --write",
		"lint": "eslint src --ext ts && tsc --noemit",
		"test": "pnpm run lint && pnpm run compile-tests && webpack && node dist/test/runTest.js"
	},
