	"configurationDefaults": {
		"snowflake-sql": {
			"editor.semanticHighlighting.enabled": true
		}
	},
	"activationEvents": [
		"onNotebook:snowbook"
	],
	"contributes": {
		"notebooks": [
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"id": "snowbook",
				"type": "snowbook",
				"displayName": "Snowbook",
				"selector": [
					{
						"filenamePattern": "*.snowbook"
					}
				],
				"icon": {
					"dark": "static/snowflake.svg",
					"light": "static/snowflake.svg"
				}
			}
		],
		"notebookRenderer": [
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"id": "snowbookRenderer",
				"displayName": "Snowflake Renderer",
				"entrypoint": "snowbook.js",
				"requiresMessaging": "optional",
				"mimeTypes": [
					"application/snow-table"
				]
			}
		],
		"configuration": {
			"title": "Snowflake",
			"properties": {
				"snowflake.queryHistory.itemLimit": {
					"type": "number",
					"default": 1000,
					"maximum": 10000,
					"description": "Specifies the number of queries shown in history. Showing more queries may affect performance."
				},
				"snowflake.showExecuteAboveStatement.enabled": {
					"type": "boolean",
					"default": true,
					"description": "Enable a clickable execution action above each statement."
				},
				"snowflake.highlightQuery.enabled": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Enable background highlight on the current SQL statement."
				},
				"snowflake.autocompleteObjectDetails.enabled": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Show Object Details upon selecting an Snowflake Object autocomplete entry."
				},
				"snowflake.autocompleteVariantKeys.enabled": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Show OBJECT/VARIANT key autocomplete suggestions"
				},
				"snowflake.setClientSessionKeepAlive.enabled": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "When true: Snowflake keeps the session active indefinitely as long as the connection is active, even if there is no activity from the user. \n\nWhen false: The user must log in again after four hours of inactivity."
				},
				"snowflake.snowsqlConfigPath": {
					"type": "string",
					"default": "",
					"markdownDescription": "If set, [connection configuration](https://docs.snowflake.com/en/user-guide/snowsql-config) will be loaded from this file.  \n\n_NOTE: Only connection configuration values will be loaded, other configuration values will be ignored.._"
				},
				"snowflake.exportCSV.quotes.enabled": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Enable double quotes around cell values in exported CSV file."
				},
				"snowflake.exportCSV.header.enabled": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Enable inclusion of header row in exported CSV file."
				},
				"snowflake.exportCSV.includeEmptyRows.enabled": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Enable inclusion of empty rows in exported CSV file."
				},
				"snowflake.exportCSV.delimiter.option": {
					"type": "string",
					"default": "Comma",
					"description": "Specifies delimiter for columns",
					"enum": [
						"Comma",
						"Tab"
					],
					"enumDescriptions": [
						"Uses commas as delimiters.",
						"Uses tabs as delimiters."
					]
				},
				"snowflakeSqlLanguageServer.trace.server": {
					"scope": "window",
					"type": "string",
					"enum": [
						"off",
						"messages",
						"verbose"
					],
					"default": "off",
					"description": "Traces the communication between VS Code and the Snowflake SQL language server."
				},
				"snowflake.enablePublicPreviewFeatures": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Enable Public Preview features for the extension"
				},
				"snowflake.skipOverwriteWarning": {
					"type": "boolean",
					"default": false,
					"markdownDescription": "Skip overwrite warning",
					"description": "By default, a warning dialog is shown when the 'overwrite=true' option is used to upload files to stages. Enable this setting to bypass the warning dialog."
				},
				"snowflake.objectExplorer.search": {
					"type": "boolean",
					"default": true,
					"markdownDescription": "Enable search in object explorer",
					"description": "Enable search in the object explorer."
				}
			}
		},
		"walkthroughs": [
			{
				"id": "snowflake.intro",
				"title": "Getting Started",
				"description": "An introduction to using the Snowflake VS Code Extension.",
				"steps": [
					{
						"id": "authenticate",
						"title": "Connect to Snowflake",
						"description": "To get started, select the Snowflake icon in the left pane and sign in to your account. Once signed in, you can use the dropdowns to select your active role, database, and warehouse.\n[Add Account](command:snowflakeAccount.focus)",
						"media": {
							"image": "static/walkthrough/signin.gif",
							"altText": "Signing in to Snowflake"
						},
						"completionEvents": [
							"onContext:snowflake.isActiveAccountLoggedIn"
						]
					},
					{
						"when": "isMac",
						"id": "execute-sql-mac",
						"title": "Write and Execute Snowflake SQL",
						"description": "Create a new .sql file or set the language of an existing file to ``Snowflake SQL`` and get writing.\n``âŒ˜ + ENTER`` will execute the statement at your cursor position or multiple statements if highlighted.\n Click the inline \"Execute\" link to execute a single statement or use the top-right \"Execute All Statements\" button to execute all statements in the current file. \n[Create Snowflake SQL File](command:snowflake.newSnowsqlFile?%5B%7B%22source%22%3A%22gettingStarted%22%7D%5D)",
						"media": {
							"image": "static/walkthrough/execute-sql.gif",
							"altText": "Execute SQL Demo"
						},
						"completionEvents": [
							"onCommand:snowflake.executeStatements"
						]
					},
					{
						"when": "isLinux || isWindows",
						"id": "execute-sql",
						"title": "Write and Execute Snowflake SQL",
						"description": "Create a new .sql file or set the language of an existing file to ``Snowflake SQL`` and get writing.\n``CTRL + ENTER`` will execute the statement at your cursor position or multiple statements if highlighted.\n Click the inline \"Execute\" link to execute a single statement or use the top-right \"Execute All Statements\" button to execute all statements in the current file. \n[Create Snowflake SQL File](command:snowflake.newSnowsqlFile?%5B%7B%22source%22%3A%22gettingStarted%22%7D%5D)",
						"media": {
							"image": "static/walkthrough/execute-sql-multiple.gif",
							"altText": "Execute SQL Demo"
						},
						"completionEvents": [
							"onCommand:snowflake.executeStatements"
						]
					},
					{
						"id": "db-explorer",
						"title": "Object Explorer",
						"description": "Use the Object Explorer to view different database types (standard, applications, application packages) and all of the objects that reside within them (schemas, functions, pipes, streams, stages, tables, views, and tasks). Clicking on an item will bring up details in the bottom pane.",
						"media": {
							"image": "static/walkthrough/obj-explorer.gif",
							"altText": "Object Explorer"
						}
					},
					{
						"id": "autocomplete",
						"title": "Autocomplete Support",
						"description": "Autocomplete support for keywords, functions, objects, and columns. Tooltips for built-in functions with help text, and link to the docs.",
						"media": {
							"image": "static/walkthrough/autocomplete.gif",
							"altText": "Autocomplete Support"
						}
					}
				]
			}
		],
		"commands": [
			{
				"command": "snowflake.executeStatements",
				"enablement": "editorTextFocus && editorLangId == 'snowflake-sql'",
				"icon": "$(play)",
				"title": "Snowflake: Execute Current Statement"
			},
			{
				"command": "snowflake.executeAllStatements",
				"enablement": "editorTextFocus && editorLangId == 'snowflake-sql'",
				"icon": "$(run-all)",
				"title": "Snowflake: Execute All Statements"
			},
			{
				"command": "snowflake.executeStatementsParallel",
				"icon": "$(run-all)",
				"title": "Snowflake: Execute Statements in Parallel"
			},
			{
				"command": "snowflake.inspectNivis",
				"enablement": "snowflake.enableNivisInspector",
				"icon": "$(type-hierarchy)",
				"title": "Nivis: Inspect"
			},
			{
				"command": "snowflake.previewNivis",
				"enablement": "snowflake.enableNivisPreview",
				"icon": "$(open-preview)",
				"title": "Nivis: View Rendered SQL"
			},
			{
				"command": "snowflake.objectDetails",
				"title": "Snowflake: View Object Details",
				"icon": "$(info)"
			},
			{
				"command": "snowflake.newSnowsqlFile",
				"title": "Snowflake SQL File",
				"icon": "$(new-file)"
			},
			{
				"command": "snowflake.refreshObjectExplorer",
				"title": "Snowflake: Refresh Object Explorer",
				"icon": "$(refresh)"
			},
			{
				"command": "snowflake.getStarted",
				"title": "Snowflake: Get Started",
				"icon": "$(question)"
			},
			{
				"command": "snowflake.rerunQueryFromHistory",
				"title": "Snowflake: Rerun Query",
				"icon": "$(debug-rerun)"
			},
			{
				"command": "snowflake.clearQueryHistory",
				"title": "Snowflake: Clear Query History",
				"icon": "$(clear-all)"
			},
			{
				"command": "snowflake.removeQuery",
				"title": "Snowflake: Remove Query",
				"icon": "$(close)"
			},
			{
				"command": "snowflake.cancelQuery",
				"title": "Snowflake: Cancel Query",
				"icon": "$(circle-slash)"
			},
			{
				"command": "snowflake.clearAccounts",
				"title": "Snowflake: Remove All Accounts"
			},
			{
				"command": "snowflake.copyToClipboard",
				"title": "Snowflake: Copy to Clipboard",
				"icon": "$(copy)"
			},
			{
				"command": "snowflake.restartLanguageServer",
				"title": "Snowflake: Restart Language Server",
				"icon": "$(debug-restart)"
			},
			{
				"command": "snowflake.revealObjectExplorerNode",
				"title": "Snowflake: Reveal Object Explorer Node"
			},
			{
				"command": "snowflake.showChangelog",
				"title": "Snowflake: Show Change Log",
				"icon": "$(list-unordered)"
			},
			{
				"command": "snowflake.openNivisDocs",
				"enablement": "config.snowflake.enablePrivatePreviewFeatures",
				"title": "Nivis: Open Docs",
				"icon": "$(book)"
			},
			{
				"command": "snowflake.updateExecutionContext",
				"title": "Snowflake: Update Execution Context"
			},
			{
				"command": "snowflake.stageDownload",
				"title": "Snowflake: GET from Stage",
				"icon": "$(cloud-download)"
			},
			{
				"command": "snowflake.stageUpload",
				"title": "Snowflake: PUT to Stage",
				"icon": "$(cloud-upload)"
			},
			{
				"command": "snowflake.stageRemove",
				"title": "Snowflake: REMOVE from Stage",
				"icon": "$(trash)"
			},
			{
				"command": "snowflake.setExecutionContext",
				"title": "Snowflake: Set Execution Context",
				"icon": "$(debug-stackframe-focused)"
			},
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"command": "snowflake.newSnowbook",
				"title": "Snowflake Notebook"
			},
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"command": "snowflake.createSnowparkEnvironment",
				"title": "Snowflake: Create Snowpark Environment"
			},
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"command": "snowflake.createSnowparkProject",
				"title": "Snowflake: Create Snowpark Project",
				"icon": "$(new-file)"
			},
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"command": "snowflake.runSnowparkProcedure",
				"title": "Snowflake: Run Snowpark Procedure"
			},
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures",
				"command": "snowflake.updateSnowparkEnvironment",
				"title": "Snowflake: Update Snowpark Environment"
			}
		],
		"grammars": [
			{
				"language": "snowflake-sql",
				"scopeName": "source.sql.snowflake",
				"path": "syntaxes/snowflake-sql.tmLanguage.json"
			}
		],
		"keybindings": [
			{
				"command": "snowflake.executeStatements",
				"key": "ctrl+enter",
				"mac": "cmd+enter",
				"when": "editorTextFocus && editorLangId == 'snowflake-sql' && !notebookCellListFocused"
			},
			{
				"command": "snowflake.objectDetails",
				"key": "alt+.",
				"mac": "alt+.",
				"when": "editorTextFocus && editorLangId == 'snowflake-sql'"
			},
			{
				"when": "config.snowflake.enablePrivatePreviewFeatures && editorLangId == 'snowflake-sql' && notebookCellListFocused",
				"command": "notebook.cell.execute",
				"key": "ctrl+enter",
				"mac": "cmd+enter"
			}
		],
		"languages": [
			{
				"id": "snowflake-sql",
				"aliases": [
					"Snowflake SQL",
					"snowflake-sql"
				],
				"extensions": [
					".sql"
				],
				"icon": {
					"dark": "static/snowflake-file.svg",
					"light": "static/snowflake-file.svg"
				},
				"configuration": "syntaxes/snowflake-sql.json"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "snowflake.cancelQuery",
					"when": "false"
				},
				{
					"command": "snowflake.copyToClipboard",
					"when": "false"
				},
				{
					"command": "snowflake.objectDetails",
					"when": "false"
				},
				{
					"command": "snowflake.removeQuery",
					"when": "false"
				},
				{
					"command": "snowflake.rerunQueryFromHistory",
					"when": "false"
				},
				{
					"command": "snowflake.setExecutionContext",
					"when": "false"
				},
				{
					"command": "snowflake.createSnowparkEnvironment",
					"when": "false"
				},
				{
					"command": "snowflake.revealObjectExplorerNode",
					"when": "false"
				},
				{
					"command": "snowflake.runSnowparkProcedure",
					"when": "false"
				},
				{
					"command": "snowflake.updateSnowparkEnvironment",
					"when": "false"
				}
			],
			"editor/context": [
				{
